Km = int(input('Введите число километров'))
m = int(input('Введите число метров'))
m *= 3.6
if Km > m:
    print('Километры в час {Km} больше, чем метры в секунду')
else:
    print('Метры в секунду {m} больше, чем километры в час')
    if Km == m:
        print('Километры в час {Km} равны метрам в секунду {m}')
    
