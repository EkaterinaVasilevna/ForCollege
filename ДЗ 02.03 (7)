x = int(input('Ввести 3-значное число '))
if x > 99 and x < 1000:
    a = x // 100
    b = x % 100
    c = b // 10
    d = b % 10
    print(a, c, d)
    f = a+c+d
    if f > 9 and f < 100:
        print(f, 'Сумма равна двухзначному числу')
    else:
        print(f, 'Сумма не равна духзначному числу')
        w = a*c*d
        if w > 99 and w < 1000:
            print(w, 'Произведение равно трехзначному числу')
        else:
            print(w, 'Произведение не равно трехзначному числу')
            if f > w:
                print('Сумма чисел больше произведения')
            else:
                print('Сумма чисел меньше произведения')
                if f % 5 == 0:
                    print('Сумма кратна пяти')
                else:
                    print('Сумма не кратна пяти')
                    if f % a == 0:
                        print('Сумма кратна числу a')
                    else:
                        print('Сумма не кратна числу a')
