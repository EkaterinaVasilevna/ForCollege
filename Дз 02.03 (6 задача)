x = int(input('Ввести 3-значное число '))
if x > 99 and x < 1000:
    a = x // 100
    b = x % 100
    c = b // 10
    d = b % 10
    print(a, c, d)
    if a==d:
        print(a, 'Подходит')
    else:
        print(a, 'не подходит')
        if c==c:
            print(c, 'Подходит')
        else:
            print(c, 'не подходит')
            if d==a:
                print(d, 'Подходит')
            else:
                print(d, 'не подходит')   
               
