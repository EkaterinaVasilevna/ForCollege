m1 = int(input('Введите массу: '))
v1 = int(input('Введите объем: '))
m2 = int(input('Введите массу: '))
v2 = int(input('Введите объем: '))
P1 = m1/v1
P2 = m2/v2
if P1 > P2:
    print({P1}, 'Плотность под номером один, больше чем плотность под номером два')
else:
    print({P2}, 'Плостноть под номером два, больше чем плотность под номером один')
    if P1 == P2:
        print({P1}, {P2}, 'равны')
